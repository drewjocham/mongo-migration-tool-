# MongoDB Configuration
MONGO_URL=mongodb://localhost:27017
MONGO_DATABASE=migration_examples

# Migration settings
MIGRATIONS_COLLECTION=schema_migrations
MIGRATIONS_PATH=./examplemigrations

# Connection pool settings
MONGO_MAX_POOL_SIZE=10
MONGO_MIN_POOL_SIZE=1
MONGO_MAX_IDLE_TIME=300
MONGO_TIMEOUT=30

# SSL/TLS settings (usually not needed for local development)
MONGO_SSL_ENABLED=false
MONGO_SSL_INSECURE=false

# AI Analysis (disabled for examples)
AI_ENABLED=false

# Google Docs Integration (disabled for examples)
GOOGLE_DOCS_ENABLED=false